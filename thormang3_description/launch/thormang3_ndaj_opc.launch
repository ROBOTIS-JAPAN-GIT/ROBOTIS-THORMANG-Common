<?xml version="1.0" ?>
<launch>
  <!-- Robot Model -->
  <param name="robot_description" command="$(find xacro)/xacro.py '$(find thormang3_description)/urdf/thormang3_ndaj.xacro'" />

  <!-- Show joint values -->
  <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher">
    <param name="use_gui" value="TRUE"/>
    <remap from="/joint_states" to="/opc/thormang3/joint_states" />
    <rosparam param="source_list">["/robotis/thormang3/joint_states"]</rosparam>
  </node>

  <!-- Show in Rviz -->
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find thormang3_description)/rviz/thormang3_ndaj_display.rviz" output="screen"/>
</launch>
